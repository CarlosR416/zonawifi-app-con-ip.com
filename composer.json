{
    "name": "appzona/appzona",
    "description": "Gestion de zona wifi con mikrotick",
    "keywords": ["mikortick", "zonawifi", "router", "psr7"],
    "license": "MIT",
    "authors": [
        {
            "name": "carlosr416",
            "email": "correo@correo.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "firebase/php-jwt": "^6.0",
        "illuminate/database": "^5.4",
        "monolog/monolog": "^1.17",
        "paragonie/sodium_compat": "^1.17",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "autoload": {
    	"psr-4": {
    		"App\\": "app"
    	}
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}
