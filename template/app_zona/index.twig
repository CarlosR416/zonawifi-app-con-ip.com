    <script>

    getToken = function(){
          var cookie = document.cookie.substr((document.cookie.search("token_api=")+10));

          if(cookie.search(";") == '-1'){
            return cookie;
          }else{
            return cookie.substr(0, cookie.search(";"));
          }
    }

    const api_token = getToken();

  
